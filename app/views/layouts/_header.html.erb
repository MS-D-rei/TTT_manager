<header class='navbar navbar-fixed-top navbar-inverse'>
  <div class='container'>
    <%= link_to 'TTT_manager', root_path, id: 'logo-app' %>
    <nav>
      <ul class='nav'>
        <% if user_signed_in? %>
        <li><%= link_to 'Users', users_path %></li>
        <li><%= link_to 'Teams', teams_path %></li>
        <li><%= link_to 'Bookmarks', bookmarks_path %></li>
        <li><%= link_to t('view.edit_profile'), edit_user_registration_path %></li>
          <% if current_user.try(:admin?) %>
            <li><%= link_to t('view.admin'), rails_admin_path %>
          <% end %>
        <li><%= link_to t('view.log_out'), destroy_user_session_path, method: :delete %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>